{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore"
        ],
        "align": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "space-before-function-paren": [
            true,
            {
                "named": "never"
            }
        ]
    },
    "rulesDirectory": []
}